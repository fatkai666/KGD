{
  "name": "kgd",
  "version": "0.1.2",
  "private": false,
  "description": "React components library simulated of antd",
  "author": "webkaige",
  "license": "MIT",
  "keywords": [
    "Component",
    "React",
    "UI"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fatkai666/KGD"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "fortawesome/react-fontawesome",
    "axios": "^0.23.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "husky": "^7.0.4",
    "react-transition-group": "^4.4.2"
  },
  "peerDependencies": {
    "react": ">= 17.0.2",
    "react-dom": ">= 17.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn run remove && yarn run build-ts && yarn run build-css",
    "test": "react-scripts test",
    "test:nowatch": "cross-env CI=true react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "node-sass ./src/styles/index.scss ./dist/index.css",
    "remove": "rimraf ./dist",
    "prepublish": "yarn run lint && yarn run build",
    "lint": "eslint --ext js,ts,tsx src --max-warnings 5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn  run lint"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-transition-group": "^4.4.3",
    "@types/storybook__addon-info": "^5.2.4",
    "font-awesome": "^4.7.0",
    "node-sass": "5",
    "react": "^17.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  }
}
